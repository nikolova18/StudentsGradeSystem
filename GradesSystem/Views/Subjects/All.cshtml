@model AllSubjectsQueryModel

@{ ViewData["Title"] = "Всички предмети"; }

<form method="get">
    <div class="row mb-3">
        <div class="form-group col-md-4">
            <div class="form-group">
            </div>
        </div>
    </div>
</form>


<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Име на предмет</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var subject in Model.SubjectNames)
        {
            <tr>
                <td>@subject</td>
            </tr>
        }
    </tbody>
</table>